"pactl set-sink-volume 1 +3%"
  XF86AudioRaiseVolume

"pactl set-sink-volume 1 -3%"
  XF86AudioLowerVolume

"pactl set-sink-mute 1 toggle"
  XF86AudioMute

"mpc toggle"
  XF86AudioPlay

"mpc next"
  XF86AudioNext

"mpc prev"
  XF86AudioPrev

"slock"
  Pause

"slock & systemctl suspend"
  Control + Pause
