"amixer sset Master 3%+"
  XF86AudioRaiseVolume

"amixer sset Master 3%-"
  XF86AudioLowerVolume

"amixer sset Master toggle"
  XF86AudioMute

"mpc toggle"
  XF86AudioPlay

"mpc next"
  XF86AudioNext

"snap"
  Print

"sleep 0.2; snap -s"
  Control + Print

"mpc prev"
  XF86AudioPrev

"slock"
  Pause

"slock & systemctl suspend"
  Control + Pause